{
  "name": "react-pwa",
  "version": "1.0.0",
  "type": "module",
  "description": "Starter kit for modern web applications",
  "homepage": "https://react-pwa-rosy.vercel.app/",
  "repository": {
    "type": "git",
    "url": "https://github.com/ShawnStewart/react-pwa"
  },
  "bugs": {
    "url": "https://github.com/ShawnStewart/react-pwa/issues"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "https-preview": "serve dist",
    "prepare": "husky install && cp -n env/.shared .env &",
    "prettier:check": "prettier --check \"{src,e2e}/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "prettier:fix": "prettier --write \"{src,e2e}/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "preview": "vite preview",
    "lint:check": "eslint \"{src,e2e}/**/*.{js,jsx,ts,tsx,json}\"",
    "lint:fix": "eslint --fix --max-warnings=0 \"{src,e2e}/**/*.{js,jsx,ts,tsx,json}\"",
    "test:e2e": "npx playwright test",
    "test:e2e:ui": "npx playwright test --ui",
    "test:unit": "vitest",
    "ts:check": "tsc --noEmit"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.1.6",
    "@radix-ui/react-visually-hidden": "^1.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.469.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-helmet-async": "^2.0.3",
    "react-hotkeys-hook": "^4.4.1",
    "react-router-dom": "^6.21.0",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@playwright/test": "^1.40.1",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.13.0",
    "https-localhost": "^4.7.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.49",
    "prettier": "3.1.1",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.18.0",
    "vite": "^5.0.9",
    "vite-plugin-pwa": "^0.17.4",
    "vitest": "^1.1.3",
    "workbox-window": "^7.3.0"
  },
  "lint-staged": {
    "{src,e2e}/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ],
    "{src,e2e}/**/*.{js,jsx,ts,tsx,json}": [
      "eslint --max-warnings=0"
    ]
  },
  "author": {
    "name": "Shawn Stewart"
  },
  "keywords": [
    "react",
    "pwa",
    "starter-kit",
    "template",
    "vite"
  ]
}
